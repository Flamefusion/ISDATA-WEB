{
  "name": "isdata",
  "description": "Isdata Application",
  "author": "Flamefusion",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "main": "electron.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:vite\" \"npm:dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env ELECTRON_START_URL=http://localhost:5173 wait-on http://localhost:5173 && .\\node_modules\\.bin\\electron .",
    "build": "npm run build:react && electron-builder --win",
    "build:react": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.12.2",
    "d3-shape": "^3.2.0",
    "electron-store": "^10.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.2.1",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "vite": "^5.2.0",
    "wait-on": "^9.0.1"
  },
  "build": {
    "appId": "com.isdata.app",
    "productName": "Isdata",
    "files": [
      "dist/**/*",
      "electron.js",
      "preload.js",
      "!**/node_modules/fsevents/**"
    ],
    "extraResources": [
      {
        "from": "backend/backend.exe",
        "to": "backend.exe"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "release"
    }
  }
}
